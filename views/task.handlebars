<div class="row center">
  <div class="col s12 m12">
    <div class="card">
      <div class="row">
        <div class="col s12 m6">
          {{#each rawData.Pictures}}
          <div class="card-image">
            <img src="{{link}}" alt="{{title}}" style="width: 100%; margin: 20px">
          </div>
          {{/each}}
        </div>
        <div class="col s12 m6">
          <div class="card-content">
            <div class="taskUserId" value={{rawData.UserId}}></div>
            <span class="card-title taskId" id={{rawData.id}} value={{rawData.id}}>Task: {{rawData.title}} </span>
            <h6>Location: {{rawData.location}}</h6>
            <h6>Intitial Price: ${{rawData.initial_price}}</h6>
            <h6>Leading Bidder: {{#each rawData}} {{Bids.Task.username}} {{/each}} </h6>
            <h6>Current Bid: ${{#each rawData.Bids}} {{bid_price}} {{/each}} </h6>
            <h6>Bid End Time: {{rawData.bid_end_time}}</h6>


            {{!-- bid --}}

            <div class="insertOptions">
              
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>


<!-- RENDER TEMPLATE AFTER EVERYTHING ELSE LOADED -->
<script src="/js/task.js"></script>